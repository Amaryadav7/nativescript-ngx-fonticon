{
    "compilerOptions": {
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "declaration": true,
        "noEmitHelpers": true,
        "noEmitOnError": true,
        "importHelpers": true,
        "types": [],
        "lib": [
            "dom",
            "es2015",
            "es2015.iterable"
        ]
    },
    "files": [
        "./references.d.ts",
        "nativescript-ngx-fonticon.ts",
        "./src/app/pipes/fonticon.pipe.ts",
        "./src/app/services/fonticon.service.ts"
    ],
    "exclude": [
        "node_modules",
        "src/node_modules",
        "bundles"
    ],
    "angularCompilerOptions": {
        "strictMetadataEmit": true,
        "skipTemplateCodegen": true
    }
}
